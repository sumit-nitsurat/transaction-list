{
    "env": {
      "browser" : true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:react/recommended"],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "error"
    },
    "parser": "babel-eslint",
    "overrides": [
        {
          "files": ["**/?(*.)+(spec|test).[jt]s?(x)", "./src/serviceWorked.js"]
        }
      ]
  }